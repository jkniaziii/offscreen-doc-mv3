{
  "name": "Offscreen Example",
  "manifest_version": 3,
  "version": "0.1",
  "background": {
    "service_worker": "js/worker.js"
  },
  "action": {
    "default_title": "Offscreen Example",
    "default_popup": "html/popup.html"
  },
  "icons": {
    "16": "images/logo16.png",
    "128": "images/logo128.png"
  },
  "permissions": ["offscreen", "storage"]
}
